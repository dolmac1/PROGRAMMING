-- 코드를 입력하세요
SELECT animal_id, name, case sex_upon_intake
                        when 'Neutered Male' then 'O'
                        when 'Spayed Female' then 'O'
                        when 'Intact Female' then 'X'
                        when 'Intact Male' then 'X' END AS '중성화'
from animal_ins;
