#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main(void)
{
	cin.tie(NULL);
	ios::sync_with_stdio(false);
	int a[5];
	int result = 0;

	for (int i = 0; i < 5;i++) {
		cin >> a[i];
		if (a[i] >= 40) {
			result = result + a[i];
		}
		else {
			result = result + 40;
		}
	}
	result = result / 5;
	cout << result << endl;
	return 0;
}
